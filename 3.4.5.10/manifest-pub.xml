<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:mailappor="http://schemas.microsoft.com/office/mailappversionoverrides/1.0" xsi:type="MailApp">
  <Id>050087F1-16B5-437B-87E5-EC673AAC2DB3</Id>
  <Version>3.4.5.10</Version>
  <ProviderName>Avaya Inc.</ProviderName>
  <DefaultLocale>en-us</DefaultLocale>
  <DisplayName DefaultValue="Avaya Meeting Scheduler">
  </DisplayName>
  <Description DefaultValue="Schedule Avaya Equinox and Avaya Spaces meetings within your Microsoft Outlook calendar.">
  </Description>
  <IconUrl DefaultValue="https://manage1.esna.com/equinox.mso/3.4.5.10/img/64.png" />
  <HighResolutionIconUrl DefaultValue="https://manage1.esna.com/equinox.mso/3.4.5.10/img/128.png" />
  <SupportUrl DefaultValue="https://support.avaya.com/" />
  <!--
  <AppDomains>
    <AppDomain>https://spaces.zang.io</AppDomain>
    <AppDomain>https://accounts.zang.io</AppDomain>
    <AppDomain>https://accounts.google.com</AppDomain>
  </AppDomains>
-->
  <Hosts>
    <Host Name="Mailbox" />
  </Hosts>
  <Requirements>
    <Sets>
      <Set Name="MailBox" MinVersion="1.1" />
    </Sets>
  </Requirements>
  <!-- Older clients that don't support add-in commands -->
  <FormSettings>
    <Form xsi:type="ItemEdit">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://manage1.esna.com/equinox.mso/3.4.5.10/app/app-pub.html" />
      </DesktopSettings>
    </Form>
  </FormSettings>
  <!-- Older clients that don't support add-in commands -->
  <!-- Restricted, ReadItem, ReadWriteItem, ReadWriteMailbox -->
  <Permissions>ReadWriteItem</Permissions>
  <Rule xsi:type="RuleCollection" Mode="Or">
    <Rule xsi:type="ItemIs" ItemType="Appointment" FormType="Edit" />
    <Rule xsi:type="ItemIs" ItemType="Appointment" FormType="Read" />
  </Rule>
  <DisableEntityHighlighting>false</DisableEntityHighlighting>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Requirements>
      <bt:Sets DefaultMinVersion="1.3">
        <bt:Set Name="Mailbox" />
      </bt:Sets>
    </Requirements>
    <Hosts>
      <Host xsi:type="MailHost">
        <DesktopFormFactor>
          <FunctionFile resid="bkgUrl" />
          <ExtensionPoint xsi:type="AppointmentOrganizerCommandSurface">
            <OfficeTab id="TabDefault">
              <Group id="grpComposeEquinox">
                <Label resid="grpEquinoxLabel" />
                <!-- Task pane button -->
                <Control xsi:type="Button" id="cmdEditPane">
                  <Label resid="cmdEditPaneLabel" />
                  <Supertip>
                    <Title resid="cmdEditPaneTitle" />
                    <Description resid="cmdEditPaneDescription" />
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="equinox16" />
                    <bt:Image size="32" resid="equinox32" />
                    <bt:Image size="80" resid="equinox80" />
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="appUrl" />
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
          <ExtensionPoint xsi:type="AppointmentAttendeeCommandSurface">
            <OfficeTab id="TabDefault">
              <Group id="grpReadEquinox">
                <Label resid="grpEquinoxLabel" />
                <!-- (PUB) Task pane button -->
                <Control xsi:type="Button" id="cmdJoin">
                  <Label resid="cmdJoinLabel" />
                  <Supertip>
                    <Title resid="cmdJoinTitle" />
                    <Description resid="cmdJoinDescription" />
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="join16" />
                    <bt:Image size="32" resid="join32" />
                    <bt:Image size="80" resid="join80" />
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="appUrl" />
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <!-- zang icon -->
        <bt:Image id="equinox16" DefaultValue="https://manage1.esna.com/equinox.mso/3.4.5.10/img/equinox16.png" />
        <bt:Image id="equinox32" DefaultValue="https://manage1.esna.com/equinox.mso/3.4.5.10/img/equinox32.png" />
        <bt:Image id="equinox80" DefaultValue="https://manage1.esna.com/equinox.mso/3.4.5.10/img/equinox80.png" />
        <!-- join icon -->
        <bt:Image id="join16" DefaultValue="https://manage1.esna.com/equinox.mso/3.4.5.10/img/join16.png?a=a" />
        <bt:Image id="join32" DefaultValue="https://manage1.esna.com/equinox.mso/3.4.5.10/img/join32.png?a=a" />
        <bt:Image id="join80" DefaultValue="https://manage1.esna.com/equinox.mso/3.4.5.10/img/join80.png?a=a" />
      </bt:Images>
      <bt:Urls>
        <bt:Url id="appUrl" DefaultValue="https://manage1.esna.com/equinox.mso/3.4.5.10/app/app-pub.html" />
        <bt:Url id="bkgUrl" DefaultValue="https://manage1.esna.com/equinox.mso/3.4.5.10/app/bkg-pub.html" />
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="grpEquinoxLabel" DefaultValue="Equinox Meeting">
        </bt:String>
        <bt:String id="cmdEditPaneLabel" DefaultValue="Meeting Details">
        </bt:String>
        <bt:String id="cmdEditPaneTitle" DefaultValue="Show Equinox Meeting panel">
        </bt:String>
        <bt:String id="cmdJoinLabel" DefaultValue="Join">
        </bt:String>
        <bt:String id="cmdJoinTitle" DefaultValue="Join Equinox Meeting">
        </bt:String>
        <bt:String id="cmdHostLabel" DefaultValue="Start">
        </bt:String>
        <bt:String id="cmdHostTitle" DefaultValue="Start Equinox Meeting">
        </bt:String>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="cmdEditPaneDescription" DefaultValue="Show Equinox Meeting panel">
        </bt:String>
        <bt:String id="cmdJoinDescription" DefaultValue="Join Equinox meeting">
        </bt:String>
        <bt:String id="cmdHostDescription" DefaultValue="Start Equinox meeting">
        </bt:String>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>